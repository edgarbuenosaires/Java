package tp1.tp3.ej05;

public class Principal {

	public static void main(String[] args) {
		Computadora c = new Computadora();
		c.agregarArchivo(new ArchivoTexto("Libro de Java", 200, "C/", true, "UTFN8"));
		c.agregarArchivo(new ArchivoTexto("Lista de Compras", 100, "D:/", true, "Alguna"));

		c.mostrarArchivos();
		System.out.println("Archivos de texto: " + c.cantidadArchivosDeTexto());
	}

}
