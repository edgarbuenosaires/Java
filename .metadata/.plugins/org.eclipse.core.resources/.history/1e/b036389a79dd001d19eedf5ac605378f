package tp1.tp3.ej13softwareTest;

import java.util.ArrayList;

public class FteLengProg extends Fuente {
	private FuentesLengProgram fuente;
	private ArrayList<Metodo> metodos;

	public FteLengProg(String nombre, String ubicacion, FuentesLengProgram fuente, ArrayList<Metodo> metodos) {
		super(nombre, ubicacion);
		this.fuente = fuente;
		this.metodos = metodos;
	}

}
