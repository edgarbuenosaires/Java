		super();
		this.nombre = nombre;
		this.peso = peso;
		this.lovalizacion = lovalizacion;
		this.abierto = abierto;
	}

	public void abrir() {

	}

	public void cerrar() {

	}

	public boolean mover(String nombre) {
		return false;
	}
}
